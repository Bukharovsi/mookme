{
  "name": "@escape.tech/mookme",
  "version": "1.1.0",
  "description": "A git hook manager designed for monorepos",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mookme": "bin/index.js"
  },
  "scripts": {
    "prebuild": "rm -f ./.tmp/ts-build-infos.json && rm -rf dist",
    "build": "tsc",
    "dev": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Escape-Technologies/mookme"
  },
  "keywords": [
    "pre",
    "commit",
    "pre-commit",
    "hook",
    "monorepos"
  ],
  "author": "lecanu.maxence@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Escape-Technologies/mookme/issues"
  },
  "homepage": "https://github.com/Escape-Technologies/mookme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-angular": "^12.1.1",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@tsconfig/node10": "^1.0.7",
    "@types/inquirer": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "markdownlint-cli": "^0.27.1",
    "prettier": "2.3.0",
    "semantic-release": "^18.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vuepress": "^1.8.2"
  },
  "dependencies": {
    "@types/node": "^15.0.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "draftlog": "^1.0.13",
    "inquirer": "^8.0.0",
    "wildcard-match": "^5.1.0"
  }
}
