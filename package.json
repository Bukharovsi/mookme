{
  "name": "@escape.tech/mookme",
  "version": "0.0.14",
  "description": "A git hook manager designed for monorepos",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mookme": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Escape-Technologies/mookme"
  },
  "keywords": [
    "pre",
    "commit",
    "pre-commit",
    "hook",
    "monorepos"
  ],
  "author": "lecanu.maxence@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Escape-Technologies/mookme/issues"
  },
  "homepage": "https://github.com/Escape-Technologies/mookme",
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/inquirer": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vuepress": "^1.8.2"
  },
  "dependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-angular": "^12.1.1",
    "@types/node": "^15.0.2",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "draftlog": "^1.0.13",
    "inquirer": "^8.0.0",
    "ora": "^5.4.0"
  },
  "mookme": {
    "packagesPath": ".",
    "packages": [
      "docs"
    ]
  }
}
