{
  "name": "@escape.tech/mookme",
  "version": "0.0.21",
  "description": "A git hook manager designed for monorepos",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mookme": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:lint": "markdownlint -c docs/.markdownlint.json docs",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Escape-Technologies/mookme"
  },
  "keywords": [
    "pre",
    "commit",
    "pre-commit",
    "hook",
    "monorepos"
  ],
  "author": "lecanu.maxence@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Escape-Technologies/mookme/issues"
  },
  "homepage": "https://github.com/Escape-Technologies/mookme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-angular": "^12.1.1"
  },
  "dependencies": {
    "@types/node": "^15.0.2",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "draftlog": "^1.0.13",
    "inquirer": "^8.0.0",
    "wildcard-match": "^5.1.0"
  },
  "mookme": {
    "packagesPath": "packages",
    "packages": [
      "docs",
      "mookme",
      "hub"
    ]
  }
}
